<?xml version="1.0" encoding="UTF-8"?>
<settings>
  <grid>
    <x>
      <alpha>10</alpha>
    </x>
    <y>
      <alpha>10</alpha>
    </y>
  </grid>
  <values>
    <x>
      <min>-300</min>
      <max>300</max>
      <strict_min_max>1</strict_min_max>
    </x>
    <y>
      <min>-150</min>
      <max>150</max>
      <strict_min_max>1</strict_min_max>
    </y>
  </values>
  <zoom>
    <enabled></enabled>
  </zoom>
  <legend>
    <enabled>false</enabled>
  </legend>
  <help>
    <balloon>
      <text>Select the area to enlarge</text>
    </balloon>
  </help>
  <bullets>
    <hover_brightness>20</hover_brightness>
    <grow_time>2</grow_time>
    <sequenced_grow>1</sequenced_grow>
  </bullets>
  <graphs>
	<graph gid="0">
	  <color>B92F2F</color>
      <title>Live in urban area</title>
      <alpha>0</alpha>
